--\set PROMPT1 '(%n@%M:%>) [%/]  [%x] %# '
\set QUIET on
\set PROMPT1 '\npsql %[%033[0;33;32m%]%/%[%033[0m%]@%[%033[0;33;33m%]%M%[%033[0m%] as %[%033[0;33;35m%]%n%[%033[0m%] %[%033[0;33;36m%]%x%[%033[0m%]%R> '
\set PROMPT2 '%R> '
\set HISTCONTROL ignoredups
\set VERBOSITY verbose
\set HISTSIZE 10000
\set COMP_KEYWORD_CASE lower
\pset linestyle unicode
\timing
\pset pager off
\pset null '(null)'
\encoding unicode
\pset format wrapped
\set ON_ERROR_ROLLBACK
\set FETCH_COUNT 10000
SET tcp_keepalives_idle = 10;

\set QUIET off
